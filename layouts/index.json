[
  {{ $post := where .Site.AllPages "Type" "section" }}
  {{ range $index, $page := $post }}
      {{ if $index }},{{ end }}
        {
            "url": {{ $page.RelPermalink | jsonify }},
            "title": {{ $page.Title | jsonify}},
            "content": {{ $page.Content | jsonify }}
        }
    {{ end }}
]
